#include <iostream>
#include <vector>

using namespace std;

int n, k, coin[109], dp[10009];

void input()
{
    cin >> n >> k;
    for(int i = 0; i < n; i++)
        cin >> coin[i];
}

int DP()
{
    dp[0] = 1;
    
    for(int i = 0; i < n; i++)
    {
        for(int j = 1; j <= k; j++)
        {
            if(j - coin[i] < 0)
                continue;
            dp[j] += dp[j - coin[i]];
        }
    }
    
    return dp[k];
}

void Solve()
{
    input();
    cout << DP();
}


int main()
{
    ios::sync_with_stdio(false);
    cin.tie(NULL); 
    cout.tie(NULL);
    
    Solve();
    
    return 0;
}
